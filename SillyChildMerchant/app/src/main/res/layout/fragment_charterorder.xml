<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    style="@style/MatchMatch"
    android:background="@color/background"
    android:orientation="vertical">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/dimen45"
        android:background="@color/whiteColors"
        android:orientation="horizontal">

        <LinearLayout
            android:id="@+id/ll_ongoing"
            style="@style/AutoMatch"
            android:gravity="center_horizontal"
            android:orientation="vertical">

            <TextView
                android:id="@+id/tv_ongoing"
                style="@style/MatchAuto"
                android:gravity="center"
                android:text="@string/ongoing"
                android:textColor="@color/tabColors"
                android:textSize="@dimen/textsize13" />

            <TextView
                android:id="@+id/tv_ongoing1"
                android:layout_width="@dimen/dimen25"
                android:layout_height="@dimen/dimen2"
                android:background="@color/whiteColors" />

        </LinearLayout>

        <LinearLayout
            android:id="@+id/ll_obligation"
            style="@style/AutoMatch"
            android:gravity="center_horizontal"
            android:orientation="vertical">

            <TextView
                android:id="@+id/tv_obligation"
                style="@style/MatchAuto"
                android:gravity="center"
                android:text="@string/obligation"
                android:textColor="@color/tabColors"
                android:textSize="@dimen/textsize13" />

            <TextView
                android:id="@+id/tv_obligation1"
                android:layout_width="@dimen/dimen25"
                android:layout_height="@dimen/dimen2"
                android:background="@color/whiteColors" />

        </LinearLayout>


        <LinearLayout
            android:id="@+id/ll_toEvaluate"
            style="@style/AutoMatch"
            android:gravity="center_horizontal"
            android:orientation="vertical">

            <TextView
                android:id="@+id/tv_toEvaluate"
                style="@style/MatchAuto"
                android:gravity="center"
                android:text="@string/toEvaluate"
                android:textColor="@color/textColor"
                android:textSize="@dimen/textsize13" />

            <TextView
                android:id="@+id/tv_toEvaluate1"
                android:layout_width="@dimen/dimen25"
                android:layout_height="@dimen/dimen2"
                android:background="@color/whiteColors" />

        </LinearLayout>


        <LinearLayout
            android:id="@+id/ll_completed"
            style="@style/AutoMatch"
            android:gravity="center_horizontal"
            android:orientation="vertical">

            <TextView
                android:id="@+id/tv_completed"
                style="@style/MatchAuto"
                android:gravity="center"
                android:text="@string/completed"
                android:textColor="@color/textColor"
                android:textSize="@dimen/textsize13" />

            <TextView
                android:id="@+id/tv_completed1"
                android:layout_width="@dimen/dimen25"
                android:layout_height="@dimen/dimen2"
                android:background="@color/whiteColors" />

        </LinearLayout>


        <LinearLayout
            android:id="@+id/ll_all"
            style="@style/AutoMatch"
            android:gravity="center_horizontal"
            android:orientation="vertical">

            <TextView
                android:id="@+id/tv_all"
                style="@style/MatchAuto"
                android:gravity="center"
                android:text="@string/all"
                android:textColor="@color/greenColors"
                android:textSize="@dimen/textsize13" />

            <TextView
                android:id="@+id/tv_all1"
                android:layout_width="@dimen/dimen25"
                android:layout_height="@dimen/dimen2"
                android:background="@color/greenColors" />

        </LinearLayout>


    </LinearLayout>

    <FrameLayout
        android:id="@+id/order_chartercontent"
        style="@style/MatchAuto"
        android:layout_marginTop="@dimen/dimen6" />

</LinearLayout>